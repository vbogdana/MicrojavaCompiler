	int uporedi(string arr1, string arr2, int len1, int len2) int i; {
		if (len1 != len2)
			return 0;
		
		i = 0;
		while (i < len1) {
			if (arr1[i] != arr2[i])
				return 0;
			i++;
		}
		
		return 1;
	}
	
