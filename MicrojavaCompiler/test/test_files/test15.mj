	void konkatenacija(string str1, int len1, string str2, int len2) char res[]; int i, j, len3; {
		len3 = len1 + len2;
		res = new char[len3];
		
		i = 0;
		while (i < len1) {
			res[i] = str1[i];
			i++;
		}
		
		j = 0;
		while (j < len2) {
			res[i] = str2[j];
			i++;
			j++;
		}
		
		// samo sam posle u parseru dodala da gura adresu res na stek

	}
	
